package ptanalysis

model ToyNormal {
  random RealVar mu ?: latentReal
  
  param Integer n ?: pow(2, 10) as int - 1
  param Matrix suffStat ?: Annealers::generatePartialSums(new Random(1), n, Normal::distribution(1000,1))
  
  laws {
    mu ~ Normal(0, 100 * 100)
    
    | n, mu, suffStat ~ LogPotential({ 
      mu * (suffStat.get(n) - 0.5 * n * mu)
    })
  }
}